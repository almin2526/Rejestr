@model IEnumerable<RejestrSzkolen.ViewModels.Statystyki2VM>

 @{
     Layout =null;
}

<div class="panel panel-default">
    <div class="panel panel-heading">Wg. imienia</div>
    
        <div class="list-group">
            @foreach (var item in Model)
            {
                <a class="list-group-item" href='@Url.Action("Index","Students",new { searchString = item.Imie})' >
                    <span class="glyphicon glyphicon-calendar"></span>
                    @Html.DisplayFor(modelItem => item.Imie)
                    <span class="badge">
                        @item.LiczbaStudentow
                    </span>
                </a>
            }
        </div>
  

</div>

 